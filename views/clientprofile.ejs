              e.preventDefault(); // prevents page reloading
              var messageElement = document.getElementById('message');
              const msg = {
                msg: messageElement.value,
                to: "<%= therapistId %>",
                from: "<%= user._id %>"
              }
              console.log("Trying to send Message", msg)
              socket.emit('chat message', JSON.stringify(msg));
