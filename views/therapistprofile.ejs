    </div>

    <div class="row">

      <!-- LOCAL INFORMATION -->
      <div class="col-sm-6">
        <div class="well">



        </div>
      </div>
      <section class="userInfo">
        ​
        <div class="">
          <img src="/<%= user.local.profileImg %>" class="propic" alt="Profile Photo">
        </div>

        <p>Bio: <%= user.local.therapistInfo.bio %></p>
        <p>Location: <%= user.local.therapistInfo.location %></p>
        <p>Phone Number: <%= user.local.phone %></p>
        <p>Email: <%= user.local.email %></p>
        <p>Current Clients: <p>
            <ul>
              <% for (let i = 0; i < clients.length; i++) { %>
              <% if (user.local.therapistClients.includes(clients[i]._id)) { %>
              <li><a href="profile/<%=clients[i]._id%>"><%= clients[i].local.name%></a></li>
              <% } %>
              <% } %>
            </ul>
            <button class="editInfo" type="button" name="button">Edit Info</button>
            ​
            <div class="editUserInfo hide">
              <form>
                <p>Name: <span><textarea class="editName"><%= user.local.name %></textarea></span></p>
                <p>Phone Number: <span><textarea class="editNumber"><%= user.local.phone %></textarea></span></p>
                <p>Bio: <span><textarea class="editBio"><%= user.local.therapistInfo.bio %></textarea></span></p>
                <span><textarea class="editEmail hide"><%= user.local.email %></textarea></span>
          </p>
          <p>Location: <span><textarea class="editLocation"><%= user.local.therapistInfo.location %></textarea></span></p>
          <button type="button" class="saveUserInfo">Save</button>
          <button type="button" class="cancelEdit">Cancel</button>
          </form>
          ​
          ​
          <form action="/uploadProPic" enctype="multipart/form-data" method="POST" class="updateProPic">
            <p>Update Profile Photo:</p>
            <input type="file" name="file-to-upload" class="floatLeft" value="">
            <input type="submit" class="uploadButton floatLeft" value=" Upload">
          </form>
    </div>
